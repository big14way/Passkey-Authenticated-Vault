{
  "chain": "stacks",
  "uuid": "vault-reputation-v1",
  "name": "Vault Reputation Events",
  "version": 1,
  "networks": {
    "testnet": {
      "start_block": 1,
      "if_this": {
        "scope": "print_event",
        "contract_identifier": "ST1PQHQKV0RJXZFY1DGX8MNSNYVE3VGZJSRTPGZGM.vault-reputation",
        "contains": "event"
      },
      "then_that": {
        "http_post": {
          "url": "http://localhost:3000/api/events/reputation",
          "authorization_header": "Bearer VAULT_API_KEY"
        }
      }
    },
    "mainnet": {
      "start_block": 1,
      "if_this": {
        "scope": "print_event",
        "contract_identifier": "SP000000000000000000002Q6VF78.vault-reputation",
        "contains": "event"
      },
      "then_that": {
        "http_post": {
          "url": "https://api.vault.com/events/reputation",
          "authorization_header": "Bearer VAULT_API_KEY"
        }
      }
    }
  },
  "expire_after_occurrence": null,
  "decode_clarity_values": true,
  "enabled": true
}
